<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Money Feet</title>

    <link href="/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="/css/bootstrap-responsive.css" rel="stylesheet" type="text/css" />
    <link href="/css/pymwyfa.css?v={{css_version}}" rel="stylesheet" type="text/css" />

    <script src="/js/jquery.min.js" type="text/javascript"></script>
    <script src="/js/bootstrap.js" type="text/javascript"></script>
    <script src="/js/date.format.js" type="text/javascript"></script>
    <script src="/js/handlebars.js" type="text/javascript"></script>
    <script src="/js/finch.min.js" type="text/javascript"></script>
    <script src="/js/pymwyfa.routing.js?v={{route_version}}" type="text/javascript"></script>
    <script src="/js/pymwyfa.other.js?v={{other_version}}" type="text/javascript"></script>

    <script type="text/javascript">
      $(function() {
        return Finch.listen();
      });
    </script>
